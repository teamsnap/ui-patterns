<!DOCTYPE html>
<html class="{{htmlclass}}">
  <head>
    {{#block "head"}}
      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1">
      {{#if title}}
        <title>{{title}} - {{data "project.title"}}</title>
      {{else if name}}
        <title>{{name}} - {{data "project.title"}}</title>
      {{else}}
        <title>{{data "project.title"}}</title>
      {{/if}}
      <link rel="stylesheet" href="https://aa5498032991a101442c-34c0f4eec246050dfc1ee92670a7b97d.ssl.cf1.rackcdn.com/new-skin/app-team-198f64681d3e8ed17ddaf2fb0cd75c92.css">    
      <link rel="stylesheet" href="{{@root.baseurl}}/assets/toolkit/styles/teamsnap-ui.css">
      {{#each styles}}
        <link rel="stylesheet" href="{{@root.baseurl}}/assets/toolkit/styles/{{this}}.css">
      {{/each}}
    {{/block}}
  </head>
  <body class="{{bodyclass}}">
    {{#block "body"}}
      {{#block "main"}}

      {{/block}}
    {{/block}}
    {{#block "foot"}}

    {{/block}}
  </body>
</html>
